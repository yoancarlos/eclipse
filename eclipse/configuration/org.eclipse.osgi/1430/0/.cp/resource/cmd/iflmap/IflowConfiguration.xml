		<DataType>xsd:string</DataType>
        <isparameterized>false</isparameterized>
		<GuiLabels>
			<Label language="EN">Product Profile</Label>
		</GuiLabels>
		<FixedValues>
			<FixedValue>
				<Value>$profile.availableProductProfiles</Value>
				<GuiLabels>
					<Label language="EN"></Label>
				</GuiLabels>
			</FixedValue>
		</FixedValues>
	</AttributeMetadata>
	<AttributeMetadata>
